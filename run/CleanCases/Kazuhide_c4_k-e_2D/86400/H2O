FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "86400";
    object      H2O;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0.01;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform 0.01;
    }
    source2
    {
        type            fixedValue;
        value           uniform 0.01;
    }
    source3
    {
        type            fixedValue;
        value           uniform 0.01;
    }
    outlet
    {
        type            inletOutlet;
        inletValue      uniform 0;
        value           uniform 0.01;
    }
    frontAndBack
    {
        type            empty;
    }
    leftAndRight
    {
        type            zeroGradient;
    }
    topAndBottom
    {
        type            zeroGradient;
    }
}


