FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      C10H16;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0.0;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           $internalField;
    }
    source2
    {
        type            fixedValue;
        value           uniform 0.0001; //0.00001384954
        //V = m*Rspec*T/P -> 0.23 mg, M 136.24, 296 K 0.3*0.01 m^2 opening
        //(((2.3*10^-7)*(8314/136.23)*296)/(1*10^5))/(0.3*0.01) = 0.00001384954 m/s @ 1 ppm?
    }
    source3
    {
        type            fixedValue;
        value           uniform 0.0001; //0.00001384954
    }
    outlet
    {
        type            inletOutlet;
        inletValue      uniform 0;
        value           uniform 0;
    }
    leftAndRight
    {
        type            zeroGradient;
    }
    topAndBottom
    {
        type            zeroGradient;
    }
    frontAndBack
    {
        type            empty;
    }
}


