FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 1e5;

boundaryField
{
    inlet
    {
      type               zeroGradient;
      value              $internalField;
    }
    source2
    {
      type               zeroGradient;
    }
    source3
    {
      type               zeroGradient;
    }
    outlet
    {
      type               totalPressure;
      p                  $internalField;
      p0                  $internalField;
      value              $internalField;
    }
    leftAndRight
    {
      type               fixedFluxPressure;
      value              $internalField;
    }
    topAndBottom
    {
      type               fixedFluxPressure;
      value              $internalField;
    }
    frontAndBack
    {
      type               empty;
    }
}


// ************************************************************************* //
